spring legacy로 생성 - spring mvc project로 진입 - 패키지 설정- jsp파일 생성
<properties>
		<java-version>11</java-version>
		<org.springframework-version>5.2.24.RELEASE</org.springframework-version>
		<org.aspectj-version>1.6.10</org.aspectj-version>
		<org.slf4j-version>1.7.36</org.slf4j-version>
	</properties>
	이렇게 변경

	pom 들어가서 java version랑  <org.aspectj-version>1.6.10</org.aspectj-version>
		랑<org.slf4j-version>1.7.36</org.slf4j-version> 변경




				<!-- https://mvnrepository.com/artifact/org.projectlombok/lombok -->
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<version>1.18.28</version>
			<scope>provided</scope>
		</dependency>

여기 이렇게 수정



<!-- Logging --> 여기 하나만 남기고 밑에 inject까지 삭제
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-api</artifactId>
			<version>${org.slf4j-version}</version>
		</dependency>



그리고
	<!-- https://mvnrepository.com/artifact/ch.qos.logback/logback-classic -->
		<dependency>
			<groupId>ch.qos.logback</groupId>
			<artifactId>logback-classic</artifactId>
			<version>1.2.12</version>
		</dependency> 여기 이렇게 수정